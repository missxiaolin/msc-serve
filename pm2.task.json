{
    "name": "fee-task-manager",
    "script": "node ./command",
    "cwd": "./",
    "args": [
        "Task:Manager"
    ],
    "watch": [
        "./commands"
    ],
    "ignore_watch": [
        "node_modules",
        "log"
    ],
    "watch_options": {
        "followSymlinks": true
    },
    "error_file": "./log/pm2/command/task-manager-err.log",
    "out_file": "./log/pm2/command/task-manager-out.log",
    "env": {
        "NODE_ENV": "development"
    },
    "env_production": {
        "NODE_ENV": "production"
    },
    "env_testing": {
        "NODE_ENV": "testing"
    }
}